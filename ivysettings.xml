<ivysettings>
  <settings defaultResolver="default"/>
  <include url="${ivy.default.settings.dir}/ivysettings-public.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-shared.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-local.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-main-chain.xml"/>
  <include url="${ivy.default.settings.dir}/ivysettings-default-chain.xml"/>
  <resolvers>
    <url name="epiceditor">
      <artifact pattern="http://[organization]/[module]/[artifact]-[revision].[ext]"/>
    </url>
    <chain name="googlecode">
      <url name="url">
        <artifact pattern="http://[module].[organization]/files/[artifact]-[revision][suffix].[ext]"/>
      </url>
      <filesystem name="fs">
        <artifact pattern="/home/xi/Downloads/[artifact]-[revision][suffix].[ext]"/>
      </filesystem>
    </chain>
  </resolvers>
  <modules>
    <module organisation="epiceditor.com" resolver="epiceditor"/>
    <module organisation="googlecode.com" resolver="googlecode"/>
  </modules>
</ivysettings>
